{Ejemplo simple de operadores}

global x;
global y;
global res;
global a;
global b;

begin
    repeat
        read x;
        read y;
        
        // Operadores relacionales
        write x > y;
        write x >= y;
        write x <= y;
        write x != y;
        write x = y;
        
        // Operadores matemÃ¡ticos
        write x + y;
        write x - y;
        write x * y;
        if y != 0 then
            write x / y
        end;
        write x % y;
        write x ** 2;
        
        // Operadores lÃ³gicos
        a := x <= y;
        b := x >= y;
        write a && b;
        write a || b;
        write !a;
        
        read res
    until res = 0
end