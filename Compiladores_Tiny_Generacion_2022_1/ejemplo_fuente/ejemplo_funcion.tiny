{Ejemplo simple de funciones}

global x;

function duplicar(num)
begin
    var temp;
    temp := num * 2;
    x := x + 1; {Modifica variable global}
    return temp
end

begin
    var resultado;
    x := 5;
    resultado := call duplicar(x);
    write resultado;
    write x {Debe mostrar 6 porque se incrementó en la función}
end

